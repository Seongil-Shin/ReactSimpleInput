{
   "name": "react-simple-material-input",
   "version": "0.1.2",
   "description": "Simple textInput element styled like material-ui.",
   "types": "dist/ReactSimpleInput.d.ts",
   "main": "dist/ReactSimpleInput.js",
   "keywords": [
      "react",
      "input",
      "simple",
      "material"
   ],
   "directories": {
      "example": "examples",
      "lib": "src/lib"
   },
   "homepage": "https://github.com/Seongil-Shin/ReactSimpleInput",
   "scripts": {
      "start": "react-scripts start",
      "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
      "build": "rm -rf ./dist && tsc && yarn copy-files",
      "prepare": "yarn build",
      "eject": "react-scripts eject"
   },
   "private": false,
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "author": {
      "name": "callmeshin75",
      "email": "callmeshin75@gmail.com",
      "url": "https://github.com/Seongil-Shin/ReactSimpleInput"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/Seongil-Shin/ReactSimpleInput"
   },
   "license": "GPL-3.0",
   "devDependencies": {
      "@types/react": "^17.0.43",
      "@types/react-dom": "^17.0.14",
      "copyfiles": "^2.4.1",
      "react": "^18.0.0",
      "react-dom": "^18.0.0",
      "react-scripts": "5.0.0",
      "rimraf": "^3.0.2",
      "typescript": "^4.6.3"
   }
}
